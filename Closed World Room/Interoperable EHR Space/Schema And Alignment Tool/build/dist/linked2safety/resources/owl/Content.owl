    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#instruction_id -->

    <owl:ObjectProperty rdf:about="&cont;instruction_id">
        <rdfs:range rdf:resource="&type;LOCATABLE_REF"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#ism_transition -->

    <owl:ObjectProperty rdf:about="&cont;ism_transition">
        <rdfs:range rdf:resource="&cont;ISM_TRANSITION"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#narrative -->

    <owl:ObjectProperty rdf:about="&cont;narrative">
