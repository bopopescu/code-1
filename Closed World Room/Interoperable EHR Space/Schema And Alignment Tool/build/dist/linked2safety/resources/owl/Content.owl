    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#wf_definition -->

    <owl:ObjectProperty rdf:about="&cont;wf_definition">
        <rdfs:range rdf:resource="&type;DV_PARSABLE"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#wf_details -->

    <owl:ObjectProperty rdf:about="&cont;wf_details">
        <rdfs:range rdf:resource="&stru;ITEM_STRUCTURE"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#work_flow_id -->

