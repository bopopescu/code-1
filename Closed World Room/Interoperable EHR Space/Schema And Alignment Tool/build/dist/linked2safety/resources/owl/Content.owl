    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#data -->

    <owl:ObjectProperty rdf:about="&cont;data">
        <rdfs:range rdf:resource="&stru;LOCATABLE"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#description -->

    <owl:ObjectProperty rdf:about="&cont;description">
        <rdfs:range rdf:resource="&stru;ITEM_STRUCTURE"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#encoding -->

    <owl:ObjectProperty rdf:about="&cont;encoding">
        <rdfs:range rdf:resource="&type;CODE_PHRASE"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#expiry_time -->

    <owl:ObjectProperty rdf:about="&cont;expiry_time">
