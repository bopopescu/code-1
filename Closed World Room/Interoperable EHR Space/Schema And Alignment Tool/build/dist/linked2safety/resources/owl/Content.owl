    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#other_participations -->

    <owl:ObjectProperty rdf:about="&cont;other_participations">
        <rdfs:range rdf:resource="&type;PARTICIPATION"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#protocol -->

    <owl:ObjectProperty rdf:about="&cont;protocol">
        <rdfs:range rdf:resource="&stru;ITEM_STRUCTURE"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#provider -->

    <owl:ObjectProperty rdf:about="&cont;provider">
