    <!-- http://www.openehr.org/owl/v1/Content#timing -->

    <owl:ObjectProperty rdf:about="&cont;timing">
        <rdfs:range rdf:resource="&type;DV_PARSABLE"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openehr.org/owl/v1/Content#transition -->

    <owl:ObjectProperty rdf:about="&cont;transition">
